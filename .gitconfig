[user]
	email = ""
	name = Traxmaxx

[core]
    excludesfile = ~/.gitignore_global
    editor = vim
    quotepath = false
    autocrlf = input
    trustExitCode = true

[color]
    branch = auto
    diff = auto
    status = auto
    interactive = auto
    ui = auto

[mergetool]
	keepBackup = trustExitCode

[alias]
    stats = shortlog -sn
    dft = difftool

[diff]
    tool = difftastic
[difftool]
    prompt = false
[difftool "difftastic"]
    cmd = difft "$LOCAL" "$REMOTE"
[pager]
    difftool = true


[push]
	default = current
[commit]
	gpgsign = false